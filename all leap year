#include <stdio.h>

int main()
{
    int y1,y2,i,k;

    printf("Enter a year1: ");
    scanf("%d",&y1);


    printf("Enter a year2: ");
    scanf("%d",&y2);

    for(i=y1;i<=y2;i++)
    // year is divisible by 400, hence the year is a leap year
if(   ((i%400==0)||(i%100!=0) ) && ( i%4==0))
        {
            printf("\n %d", i);

            k++; /* if there is leap year k!=0 */
        }

   if(k==0)
   {printf("It means there is no leapyears ");}
getch();
}
